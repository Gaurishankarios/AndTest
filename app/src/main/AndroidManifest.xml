<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="newcandor.demo.com.newcandor">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ChangePassword"></activity>
        <activity android:name=".AddNewProperty" />
        <activity android:name=".RentalDetailView" />
        <activity android:name=".SoldPropertyDetails" />
        <activity android:name=".ForgetPasswordPage" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginPage" />
        <activity android:name=".RegistrationData" />
        <activity android:name=".RegistrationDoc" />
        <activity android:name=".SoldPropertiesActivity" />
    </application>

</manifest>